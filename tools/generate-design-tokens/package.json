{
  "name": "@perseus/generate-design-tokens",
  "version": "1.0.0",
  "description": "CLI that generates React icon components from Figma",
  "author": "Kasper Kristensen <kasper@medusajs.com>",
  "bin": {
    "perseus-generate": "cli.js"
  },
  "exports": {
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist",
    "!**/*.d.ts.map"
  ],
  "scripts": {
    "build": "concurrently \"npm run build:esm\" \"npm run build:cjs\"",
    "build:esm": "tsc --project ./tsconfig.json --module ES2015 --outDir ./dist/esm",
    "build:cjs": "tsc --project ./tsconfig.json --module CommonJS --outDir ./dist/cjs"
  },
  "dependencies": {
    "@svgr/core": "^6.5.1",
    "@svgr/plugin-jsx": "^6.5.1",
    "@svgr/plugin-prettier": "^6.5.1",
    "@svgr/plugin-svgo": "^6.5.1",
    "axios": "^1.2.1",
    "cli-progress": "^3.11.2",
    "commander": "^9.4.1",
    "dotenv": "^16.0.3",
    "fs-extra": "^11.1.0",
    "picocolors": "^1.0.0",
    "pretty-error": "^4.0.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/fs-extra": "^9.0.13",
    "concurrently": "^7.6.0",
    "typescript": "^4.9.4"
  },
  "private": true,
  "license": "MIT"
}
